      <update_rate>5</update_rate>
      <imu_link>imu_link</imu_link>
      <posDriftFreq>500.0</posDriftFreq>  
      <gaussianNoise>0.0</gaussianNoise>  
      <posBiasStd>0.25</posBiasStd>  
      <posDriftStd>1.5</posDriftStd>  
      <velErrStd>0.10</velErrStd>  
    </plugin>
        
